{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "es6",
    "target": "es2015",
    "moduleResolution": "node",
    "jsx": "preserve",
    "noImplicitAny": true,
    "removeComments": false,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": true
  },
  "awesomeTypescriptLoaderOptions": {
    "useBabel": true,
    "useCache": true
  }
}
